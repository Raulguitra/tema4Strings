
package prueba;

import java.util.Scanner;
public class prueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n;
		int contadivisores;
		int k;
		 
		System.out.println("introduzca el un numero");
		n=sc.nextInt();
		
		while(n<=0 || n > 100){
			
			System.out.println("introduzca un numero entre 1 y 100");
			n=sc.nextInt();
		
			
		}//end while
		contadivisores =0;
		for (k=1; k<=n; k++){
			if(n%k==0){
			contadivisores= contadivisores++;
			
			System.out.println(contadivisores);
		}//end for
		

		
		

	}

}
