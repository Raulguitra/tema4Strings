
package prueba;

import java.util.Scanner;
public class prueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n;
	
		int k;
		int conta;
		 
		System.out.println("introduzca el un numero");
		n=sc.nextInt();
		
		while(n<=0 || n > 100){
			
			System.out.println("introduzca un numero entre 1 y 100");
			n=sc.nextInt();
		
			
		}//end while
		conta = 0;
		for (k=2; k<=n-1; k++){
			if(n%k==0){
				conta = conta +1;
				
			}//end if
			
		}//end for
		if(conta==0){
		System.out.println(" el numero " + n + " es tu primo");
		}
		if(conta!=0){
			System.out.println(" el numero " + n + " no es tu primo");
		}
	
		

		
		

	}

}
