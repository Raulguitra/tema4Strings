import java.util.Scanner;
public class ejercicio4vectores {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		double [][] valores= new double [5][5];
		int maximo= valores[0][0];
		int minimo= valores[0][0];
		 Scanner sc = new Scanner(System.in);
		 
		 System.out.println("introduce 5 numeros");
		 for(double i=1; i<5; i++){
			 System.out.println("numero");
				valores[i] =sc.nextInt();
				if(valores[i][i]>maximo){
					 maximo=valores[i][i];				 
		 }else if(valores[i][i]<minimo){
			 minimo=valores[i][i];
		 }
		 }
		 System.out.println("introduce 5 numeros");
		 for(int j=1; j<5; j++){
			 System.out.println("numero");
				valores[j] =sc.nextInt();
				if(valores[j]>maximo){
					 maximo=valores[j];
				} else if(valores[j]<minimo){
					 minimo=valores[j];
				 }
		
		 }
		 System.out.println( "el maximo es" + maximo + " y el minimo es " + minimo  );
	}

}
