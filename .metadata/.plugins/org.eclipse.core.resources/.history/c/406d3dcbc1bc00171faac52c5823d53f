		while (s1.length() > 8){
			
			System.out.println( "teclee una frase menor de 8 caracteres");
			s1=teclado.nextLine();
		}//end while
		
		
		
		
		
		if(s1.charAt(0) && s1.charAt(3)){
		}//end if
		
			while(s1.charAt(i)>=48 && s1.charAt(i)<=57){
				System.out.println( "los cuatro primeros digitos deben ser numeros");
