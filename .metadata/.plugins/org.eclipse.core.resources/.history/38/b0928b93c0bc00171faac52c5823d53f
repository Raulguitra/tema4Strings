import java.util.Scanner;
public class string9 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
  
		
		String s1;
		int i;

		Scanner teclado = new Scanner(System.in);
		System.out.println( "teclee una frase");
		s1=teclado.nextLine();
		
		while (s1.length() > 8){
			
			System.out.println( "teclee una frase menor de 8 caracteres");
			s1=teclado.nextLine();
		}//end while
		
		
		
		
		
		if(s1.charAt(0) && s1.charAt(3)){
		}//end if
		
			while(s1.charAt(i)>=48 && s1.charAt(i)<=57){
				System.out.println( "los cuatro primeros digitos deben ser numeros");
				s1=teclado.nextLine();
			}//end while




	}// end main
}//end class