import java.util.Scanner;
public class prueba {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		// leer dos vectores de dimension 3 e imprimir un tercero que sea la suma de estos dos
		
		int Array1[];
		Array1=new int[10];
		int Array2[];
		Array2=new int[10];
		int Array3[];
		Array3=new int[10];
		int i;
		int suma=0;
		
		System.out.println("Introduzca 10 numeros");
		for (i=0; i<10; i++){
			Array1[i] = sc.nextInt();
			System.out.println("otro");
		}
		System.out.println("Introduzca 10 numeros");
		for (i=0; i<10; i++){
			Array2[i] = sc.nextInt();
			System.out.println("otro");
		}
		for (i=0; i<10; i++){
		Array3[i]=Array1[i]+Array2[i];
		System.out.print(Array3[i]);
		}
		
}
}
